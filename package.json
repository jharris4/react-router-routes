{
  "name": "react-router-routes",
  "version": "1.0.0",
  "description": "Static route config matching for React Router",
  "repository": "jharris4/react-router-routes",
  "license": "MIT",
  "authors": [
    "Ryan Florence",
    "Jon Harris"
  ],
  "files": [
